    <div data-intro="#results-panel-auth" data-sequence="7" class="output-size panel results-panel">    
        <div class="results-panel-header panel-heading">
            <h3>Results Panel</h3>
        </div>
        <div class="panel-body">
            <div id="query-results"></div>
            <div id="tool-suite-panel"></div>
            <div id="filter-list-panel"></div>
            <div id="studies-list-panel" data-intro="#open-filtered-by-study" data-sequence="13"></div>
        </div>
    </div>
</div>