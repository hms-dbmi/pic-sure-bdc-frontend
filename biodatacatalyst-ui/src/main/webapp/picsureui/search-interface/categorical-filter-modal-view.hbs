<div class="filter-modal-container">
    <div class="row">
        <div class="value-container">
            {{#each searchResult.result.values}}
                <input class="categorical-filter-input" type="checkbox" value="{{this}}" tabindex="{{tabindex @index}}"/>{{this}} <br/>
            {{/each}}
        </div>

        <div class="button-container">
            <button id="add-filter-button" tabindex="1099999">Add Filter to Query</button>
        </div>
    </div>
</div>