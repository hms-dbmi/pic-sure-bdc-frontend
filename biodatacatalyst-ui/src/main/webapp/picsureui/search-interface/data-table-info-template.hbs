<div class="study-info">
    <div class="study-description">
        Study Name: {{studyDescription}}
    </div>
    <div class="study-accession">
        Study Accession: {{studyAccession}}
        <div class="badge {{colorClass this.tag}}">
        <span id="tag-{{studyAccessionTagId}}"> {{studyAccessionTagName}}
            <span data-tag="{{studyAccessionTagId}}" data-action="requireTag" class="require-tag-btn hover-control">+</span>
            <span data-tag="{{studyAccessionTagId}}" data-action="excludeTag" class="exclude-tag-btn hover-control">-</span>
        </span>
        </div>
    </div>
</div>

<div class="dataset-info">
    <div class="dataset-name">
        Dataset Name: {{variableMetadata.dataTableName}}
    </div>
    <div class="dataset-accession">
        Dataset Accession: {{variableMetadata.dataTableId}}
        <i data-target="datatable" data-datatable-id="{{variableMetadata.dataTableId}}" class="fa fa-filter search-result-action-btn"></i>
        <i data-target="datatable" data-datatable-id="{{variableMetadata.dataTableId}}" class="fa fa-database search-result-action-btn"></i>
    </div>
    <div class="dataset-description">
        Dataset Description: {{variableMetadata.dataTableDescription}}
    </div>
</div>

<div class="variable-info">
    {{#if variableId}}
        <div>
            Variable Accession: {{variableId}}
            <i data-target="variable" data-datatable-id="{{variableId}}" class="fa fa-filter search-result-action-btn"></i>
            <i data-target="variable" data-datatable-id="{{variableId}}" class="fa fa-database search-result-action-btn"></i>
        </div>
    {{/if}}
    {{#if variableMetadata.description}}
        <div>Variable Description: {{variableMetadata.description}}</div>
    {{/if}}
    {{#if variableMetadata.name}}
        <div>Variable Name: {{variableMetadata.name}}</div>
    {{/if}}
    {{#if variableMetadata.type}}
        <div>Variable Type: {{variableMetadata.type}}</div>
    {{/if}}
    {{#if variableMetadata.var_report_description}}
        <div>Variable Report Description: {{variableMetadata.var_report_description}}</div>
    {{/if}}
    {{#if variableMetadata.var_report_comment}}
        <div>Variable Report Comment: {{variableMetadata.var_report_comment}}</div>
    {{/if}}
</div>