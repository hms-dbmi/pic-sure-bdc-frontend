<style>
    .glyphicon.export-icon {
        font-size: 1.3em;
    }
</style>
<div class="variable-info">
    {{#if variableMetadata.columnmeta_name}}
        <div>Variable Name: {{variableMetadata.columnmeta_name}}</div>
    {{/if}}
    {{#if variableId}}
        <div>
            Variable Accession: {{variableMetadata.columnmeta_var_id}}
            <i data-target="variable" data-id="{{variableId}}" aria-label="Filter By Variable Button - Click to configure a filter using this variable." title="Click to configure a filter using this variable." tabindex=1000000 title="Click to configure a filter on this variable." class="fa fa-filter search-result-action-btn"></i>
            <i data-target="variable" data-id="{{variableId}}" aria-label="Add Variable to Export Button - Click to add this variable to your export." title="Click to add this variable to your export." tabindex=1000001 title="Click to add this variable to your data retrieval." class="glyphicon glyphicon-log-out export-icon search-result-action-btn"></i>
        </div>
    {{/if}}
    {{#if variableMetadata.columnmeta_description}}
        <div>Variable Description: {{variableMetadata.columnmeta_description}}</div>
    {{/if}}
    {{#if variableMetadata.columnmeta_data_type}}
        <div>Variable Type: {{variableMetadata.columnmeta_data_type}}</div>
    {{/if}}
    {{#if variableMetadata.columnmeta_var_group_description}}
        <div>Variable Report Description: {{variableMetadata.columnmeta_var_group_description}}</div>
    {{/if}}
    {{#if variableMetadata.columnmeta_var_report_comment}}
        <div>Variable Report Comment: {{variableMetadata.columnmeta_var_report_comment}}</div>
    {{/if}}
</div>
<hr>
<div class="dataset-info">
    <div class="dataset-name">
        Dataset Name: {{variableMetadata.columnmeta_name}}
    </div>
    <div class="dataset-accession">
        Dataset Accession: {{variableMetadata.columnmeta_var_group_id}}
        <i data-target="datatable" data-id="{{variableMetadata.columnmeta_var_group_id}}" tabindex=1000002 title="Click to configure a filter on this variable." class="fa fa-filter search-result-action-btn"></i>
        <i data-target="datatable" data-id="{{variableMetadata.columnmeta_var_group_id}}" tabindex=1000003 title="Click to add this variable to your data retrieval." class="glyphicon glyphicon-log-out export-icon search-result-action-btn"></i>
    </div>
    {{#if variableMetadata.columnmeta_var_group_description}}
    <div class="dataset-description">
        Dataset Description: {{variableMetadata.columnmeta_var_group_description}}
    </div>
    {{/if}}
</div>
<hr>
<div class="study-info">
    <div class="study-description">
        Study Name: {{studyDescription}}
    </div>
    <div class="study-accession">
        Study Accession: {{studyAccession}}
        <div class="badge {{colorClass this.tag}}" tabindex=1000004>
        <span id="tag-{{studyAccessionTagId}}"> {{studyAccessionTagName}}
            <span data-tag="{{studyAccessionTagId}}" data-action="requireTag" class="require-tag-btn hover-control">+</span>
            <span data-tag="{{studyAccessionTagId}}" data-action="excludeTag" class="exclude-tag-btn hover-control">-</span>
        </span>
        </div>
    </div>
</div>
